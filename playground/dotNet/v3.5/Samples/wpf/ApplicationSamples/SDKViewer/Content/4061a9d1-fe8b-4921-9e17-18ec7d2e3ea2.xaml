      <TableRowGroup>
        <TableRow>
          <TableCell>
            <Paragraph FontSize="22"><Run>XAML </Run>

            </Paragraph>

          </TableCell>

        </TableRow>

        <TableRow>
          <TableCell>
            <Paragraph xml:space="preserve" TextAlignment="Left" FontSize="8pt" FontFamily="Courier New"><Run>
&lt;Paragraph FontFamily="Pericles" Typography.DiscretionaryLigatures="True"> 
&lt;Run Typography.StylisticAlternates="1">CO
&lt;/Run> 
&lt;Run Typography.StylisticAlternates="1">LA
&lt;/Run> 
&lt;Run Typography.StylisticAlternates="1">LE
&lt;/Run> 
&lt;Run Typography.StylisticAlternates="1">LI
&lt;/Run> 
&lt;Run Typography.StylisticAlternates="1">LL
&lt;/Run> 
&lt;Run Typography.StylisticAlternates="1">LO
&lt;/Run> 
&lt;Run Typography.StylisticAlternates="1">LU
&lt;/Run> 
&lt;/Paragraph> </Run>

            </Paragraph>

          </TableCell>

        </TableRow>

      </TableRowGroup>

