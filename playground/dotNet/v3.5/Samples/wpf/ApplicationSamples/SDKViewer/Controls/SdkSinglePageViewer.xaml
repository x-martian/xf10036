          </DrawingBrush>
          <Style x:Key="{x:Type cc:SdkSinglePageViewer}" TargetType="{x:Type cc:SdkSinglePageViewer}">
            <Setter Property="MinZoom" Value="80" />
            <Setter Property="MaxZoom" Value="200" />
            <Setter Property="ZoomIncrement" Value="20" />
            <Setter Property="KeyboardNavigation.TabNavigation" Value="Cycle" />
            <Setter Property="Template">
              <Setter.Value>
                <ControlTemplate TargetType="{x:Type cc:SdkSinglePageViewer}">
                  <Border Background="White" CornerRadius="5" BorderBrush="#e4e4e4" BorderThickness="1">
                    <Grid>
                      <Grid.RowDefinitions>
                        <RowDefinition Height="*" />
                        <RowDefinition Height="Auto" />
                      </Grid.RowDefinitions>
                      <!-- Page content and top toolbar -->
                      <!-- Bottom Toolbar -->
                      <!-- This defines the Content area and its paging/scrolling controls inside the Grid -->
                      <AdornerDecorator Margin="0" Grid.Row="0">
                        <DocumentPageView Margin="0" PageNumber="0" DocumentViewerBase.IsMasterPage="True" />
                      </AdornerDecorator>
                      <DockPanel x:Name="TopToolBar" Grid.Row="0" Opacity=".1" LastChildFill="False" VerticalAlignment="Top" HorizontalAlignment="Stretch" SnapsToDevicePixels="True" Margin="0,5,5,0">
                        <!-- SaveAs Button -->
                        <Button Name="DocumentSaveAs" Click="OnSaveDocumentAs" ToolTip="Save Document As..." Background="Transparent" BorderBrush="Transparent" Padding="0" Margin="3,0,0,0" BorderThickness="0" Width="15" Height="15" DockPanel.Dock="Right" VerticalAlignment="Center">
                          <Image Stretch="Fill" Opacity=".8" Source="\Save.bmp" />
                        </Button>
                        <!-- Print Button -->
                        <Button Name="DocumentPrint" Click="PrintDocument" Background="Transparent" BorderBrush="Transparent" ToolTip="Print..." BorderThickness="0" Margin="3,0,5,0" Padding="0" Width="20" Height="20" DockPanel.Dock="Right" VerticalAlignment="Center">
                          <Grid>
                            <Viewbox Stretch="Fill">
                              <Rectangle Fill="{StaticResource PrinterIcon}" Height="1" Width="1" />
                            </Viewbox>
                          </Grid>
                        </Button>
                        <!-- Annotations -->
                        <Menu Name="DocumentAnnotations" DockPanel.Dock="Right" VerticalAlignment="Center" FontFamily="Calibri" FontSize="8pt" Background="Transparent" BorderBrush="Transparent" ToolTip="Annotate...">
                          <MenuItem Header="_Annotate">
                            <MenuItem IsCheckable="true" Name="viewAnnotationMenuItem" IsChecked="True" Header="_View Annotations" Checked="OnViewAnnotationChecked" Unchecked="OnViewAnnotationUnchecked" />
                            <Separator />
                            <MenuItem Command="ANN:AnnotationService.CreateHighlightCommand" />
                            <MenuItem Command="ANN:AnnotationService.CreateTextStickyNoteCommand" />
                            <MenuItem Command="ANN:AnnotationService.CreateInkStickyNoteCommand" />
                            <Separator />
                            <MenuItem Command="ANN:AnnotationService.ClearHighlightsCommand" />
                            <MenuItem Command="ANN:AnnotationService.DeleteStickyNotesCommand" />
                            <MenuItem Command="ANN:AnnotationService.DeleteAnnotationsCommand" />
                          </MenuItem>
                        </Menu>
                      </DockPanel>
                      <!-- Zoom control -->
                      <Grid Grid.Row="1" Margin="0,0,0,5">
                        <Grid x:Name="ZoomerControl" Width="150" HorizontalAlignment="Right" MaxWidth="250" Opacity="0.1">
                          <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="Auto" />
                          </Grid.ColumnDefinitions>
                          <Grid.RenderTransform>
                            <ScaleTransform ScaleX="0.75" ScaleY="0.75" />
                          </Grid.RenderTransform>
                          <!-- Column for PreviosPage control -->
                          <!-- Column for Zoomcontrol -->
                          <!-- Zoom Out -->
                          <Button Grid.Column="0" Style="{DynamicResource PFThemeSPVZoomOutButtonStyle}" Command="NavigationCommands.DecreaseZoom" IsEnabled="{Binding Path=CanDecreaseZoom, RelativeSource={RelativeSource TemplatedParent}}" Margin="5,0,5,0" />
                          <!-- Zoom Slider -->
                          <Slider HorizontalAlignment="Stretch" VerticalAlignment="Center" Grid.Column="1" Value="{Binding Path=Zoom,Mode=TwoWay,RelativeSource={RelativeSource TemplatedParent}}" Minimum="{Binding Path=MinZoom,RelativeSource={RelativeSource TemplatedParent}}" Maximum="{Binding Path=MaxZoom,RelativeSource={RelativeSource TemplatedParent}}" LargeChange="{Binding Path=ZoomIncrement,RelativeSource={RelativeSource TemplatedParent}}" SmallChange="{Binding Path=ZoomIncrement,RelativeSource={RelativeSource TemplatedParent}}" TickFrequency="{Binding Path=ZoomIncrement,RelativeSource={RelativeSource TemplatedParent}}" IsSnapToTickEnabled="True" Width="Auto" MinWidth="0" />
                          <!-- Zoom In -->
                          <Button Grid.Column="2" Style="{DynamicResource PFThemeSPVZoomInButtonStyle}" Command="NavigationCommands.IncreaseZoom" IsEnabled="{Binding Path=CanIncreaseZoom, RelativeSource={RelativeSource TemplatedParent}}" Margin="5,0,10,0" />
                        </Grid>
                        <!-- Page count and Nav buttons -->
                        <DockPanel HorizontalAlignment="Center" Margin="0">
                          <!-- First Page -->
                          <Button Name="FirstPageButton" Style="{DynamicResource PFThemeSPVFirstPageButtonStyle}" VerticalAlignment="Stretch" Command="NavigationCommands.FirstPage" IsEnabled="{Binding Path=CanGoToPreviousPage,RelativeSource={RelativeSource TemplatedParent}}"> 9 </Button>
                          <!-- Previous Page -->
                          <Button Name="PreviousPageButton" Style="{DynamicResource PFThemeSPVPrevPageButtonStyle}" VerticalAlignment="Stretch" Command="NavigationCommands.PreviousPage" IsEnabled="{Binding Path=CanGoToPreviousPage,RelativeSource={RelativeSource TemplatedParent}}"> 3 </Button>
                          <!-- Page Count -->
                          <TextBlock FontFamily="Tahoma" FontSize="8pt" HorizontalAlignment="Center" VerticalAlignment="Center">
                            <TextBlock Grid.Column="0" Grid.Row="0" Margin="4,0,8,0" Text="{Binding Path=MasterPageNumber,RelativeSource={RelativeSource TemplatedParent}}" /> of <TextBlock Grid.Column="2" Margin="8,0,4,0" Text="{Binding Path=PageCount,RelativeSource={RelativeSource TemplatedParent}}" />
                          </TextBlock>
                          <!-- Next Page -->
                          <Button Name="NextPageButton" Style="{DynamicResource PFThemeSPVNextPageButtonStyle}" VerticalAlignment="Stretch" Command="NavigationCommands.NextPage" IsEnabled="{Binding Path=CanGoToNextPage,RelativeSource={RelativeSource TemplatedParent}}"> 4 </Button>
                          <!-- Last Page -->
                          <Button Name="LastPageButton" Style="{DynamicResource PFThemeSPVLastPageButtonStyle}" VerticalAlignment="Stretch" Command="NavigationCommands.LastPage" IsEnabled="{Binding Path=CanGoToNextPage,RelativeSource={RelativeSource TemplatedParent}}"> : </Button>
                        </DockPanel>
                      </Grid>
                      <!-- Find ToolBar -->
                      <Border Grid.Row="0" x:Name="PART_FindToolBarHost" Visibility="Collapsed" Opacity="0.75" VerticalAlignment="Bottom" />
                      <!-- print dialog -->
                      <cc:SdkPrintDialog x:Name="printDialog" />
                    </Grid>
                  </Border>
                  <ControlTemplate.Triggers>
                    <EventTrigger RoutedEvent="Mouse.MouseEnter">
                      <EventTrigger.Actions>
                        <BeginStoryboard>
                          <BeginStoryboard.Storyboard>
                            <Storyboard>
                              <DoubleAnimation Storyboard.TargetName="TopToolBar" Storyboard.TargetProperty="Opacity" Duration="0:0:0.1" To="1" />
                              <DoubleAnimation Storyboard.TargetName="ZoomerControl" Storyboard.TargetProperty="Opacity" Duration="0:0:0.2" To="1" />
                            </Storyboard>
                          </BeginStoryboard.Storyboard>
                        </BeginStoryboard>
                      </EventTrigger.Actions>
                    </EventTrigger>
                    <EventTrigger RoutedEvent="Mouse.MouseLeave">
                      <EventTrigger.Actions>
                        <BeginStoryboard>
                          <BeginStoryboard.Storyboard>
                            <Storyboard>
                              <DoubleAnimation Storyboard.TargetName="TopToolBar" Storyboard.TargetProperty="Opacity" Duration="0:0:0.2" To="0.1" />
                              <DoubleAnimation Storyboard.TargetName="ZoomerControl" Storyboard.TargetProperty="Opacity" Duration="0:0:0.4" To="0.1" />
                            </Storyboard>
                          </BeginStoryboard.Storyboard>
                        </BeginStoryboard>
                      </EventTrigger.Actions>
                    </EventTrigger>
                  </ControlTemplate.Triggers>
                </ControlTemplate>
              </Setter.Value>
            </Setter>
          </Style>          
        </ResourceDictionary>
      </ResourceDictionary.MergedDictionaries>
    </ResourceDictionary>
    </FlowDocumentPageViewer.Resources>
</FlowDocumentPageViewer>